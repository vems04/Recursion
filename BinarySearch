import java.util.*;
public class Recursion {
    public static int vems(int x){
        if(x<2){
            return (x);
        }
    return vems(x-1)+vems(x-2);
    }
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        System.out.print(vems(x));



    }

}
